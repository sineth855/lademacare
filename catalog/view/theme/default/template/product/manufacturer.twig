{{ header }}
<!-- breadcrumb -->
<section id="page-title" class="page-title dark-page-title">
    <div class="container">
        <div class="region region-page-title">
            <div id="block-martis-page-title" class="norm-width block-title-1 block-title-left block block-core block-page-title-block">
                <div class="container-wrap clearfix">
                    <div class="block-content clearfix">
                        <h1>{{ heading_title }}</h1>
                    </div>
                </div>
            </div>
        </div>

        <div class="pull-left region region-breadcrumb">
            <div id="block-martis-breadcrumbs" class="norm-width block-title-1 block-title-left block block-system block-system-breadcrumb-block">
                <div class="container-wrap clearfix">
                    <div class="block-content clearfix">
                        <nav class="breadcrumb" aria-labelledby="system-breadcrumb">
                            <!-- <h2 id="system-breadcrumb" class="visually-hidden">Breadcrumb</h2> -->
                            <ul>
                                {% for breadcrumb in breadcrumbs %}
                                <li>
                                    <a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
                                </li>
                                {% endfor %}
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="main-container gray-bg">
    <div id="product-manufacturer" class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="full-listing-wrap">
                    <div class="listing-content-main">
                        <div class="listing-info-left">
                            <div class="main-listing-wrap">
                                <div class="heading">Full Brand Listing</div>
                                <div class="list-content">
                                    {% if categories %} {% for category in categories %} {% if category.manufacturer %} {% for manufacturers in category.manufacturer|batch(4) %}
                                    <ul>
                                        {% for manufacturer in manufacturers %}
                                        <li class="list"> <a href="{{ manufacturer.href }}" title="{{ manufacturer.name }}">{{ manufacturer.name }}</a> </li>
                                        {% endfor %}
                                    </ul>
                                    {% endfor %} {% endif %} {% endfor %} {% endif %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-lg-6">
                <div class="listing-info-right">
                    <div class="mid-ad-wrap">
                        <a href="" title="side"> <img class="ad" src="https://bucket.fitwhey.com/Banners/a3e4fd2a8bfe0f3574dd08bf05c85660.jpg" alt="ad-banner"> </a>
                    </div>
                </div>
            </div>

        </div>
    </div>

</div>
{{ footer }}