<script>
     $(".userRegisterPopUp").hide();        
    $("#modal_containerPopUp").removeClass("modal_container_class");
    $("#close").click(function() {
        $("#modal_containerPopUp").removeClass("modal_container_class");
        $(".userRegisterPopUp").hide();
    });
    $("#buttonRegisterPopUp").click(function() {
        $("#modal_containerPopUp").addClass("modal_container_class");
        $(".userRegisterPopUp").show();
    });
    jQuery(function($) {
        
        $('#retroclockbox_lg').flipcountdown({
            size: 'lg'
        });
        $('#retroclockbox_md').flipcountdown({
            size: 'md'
        });
        $('#retroclockbox_sm').flipcountdown({
            size: 'sm'
        });
        $('#retroclockbox_xs').flipcountdown({
            size: 'xs'
        });
    })
</script>

